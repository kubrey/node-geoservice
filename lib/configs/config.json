{
  "services": {
    "maxmind-dat": {
      "type": "local",
      "dbfile": ["directory"],
      "directory":"node_modules/geoip-lite/data/",
      "active": true,
      "priority": 1,
      "v6":true
    },
    "maxmind-mmdb": {
      "type": "local",
      "dbfile": "node_modules/maxmind-db-reader/test/data/GeoLite2-City.mmdb",
      "active": true,
      "priority": 30,
      "v6":true
    },
    "ipinfo": {
      "type": "service",
      "active": true,
      "priority": 25,
      "v6":true,
      "requestOptions": {
        "host": "ipinfo.io",
        "method": "GET",
        "port": 80,
        "path": "/{{ip}}"
      }
    },
    "freegeoip": {
      "type": "service",
      "active": true,
      "priority": 20,
      "v6":false,
      "requestOptions": {
        "host": "freegeoip.net",
        "port": 80,
        "path": "/json/{{ip}}",
        "method": "GET"
      }
    },
    "ip-api": {
      "type": "service",
      "active": true,
      "priority": 10,
      "v6":false,
      "requestOptions": {
        "host": "ip-api.com",
        "path": "/json/{{ip}}",
        "port": 80,
        "method": "GET"
      }
    },
    "ipgeobase": {
      "type": "local",
      "active": true,
      "priority": 2,
      "v6":false,
      "dbfile": [
        "cidrdb",
        "citiesdb"
      ],
      "cidrdb": "data/cidr_optim.txt",
      "citiesdb": "data/cities.txt",
      "countries": [
        "ua",
        "ru"
      ]
    },
    "geobytes": {
      "type": "service",
      "active": true,
      "priority": 50,
      "v6":false,
      "requestOptions": {
        "host": "getcitydetails.geobytes.com",
        "path": "/GetCityDetails?fqcn={{ip}}",
        "port": 80,
        "method": "GET"
      }
    }
  },
  "geoObject": {
    "city": false,
    "countryCode": true,
    "countryName": false,
    "latitude": false,
    "longitude": false,
    "isp": false,
    "zip": false,
    "regionName": false,
    "regionCode": false
  },
  "commonOptions": {
    "checkLevel": 1,
    "checkField": "countryCode",
    "timeout":3000
  },
  "projectName": "GeoSearch"
}